<?xml version="1.0"?>
<robot name="multipleshapes">
  <link name="base_link">
    <visual>
      <geometry>
        <cylinder length="0.6" radius="0.2"/>
      </geometry>
      <origin rpy="0 0 0" xyz="-1 -1 0.3"/>
    </visual>
  </link>
   
  <link name="right_leg">
    <visual>
      <geometry>
        <box size="0.6 0.5 1"/>
      </geometry>
      <origin rpy="0 0 0" xyz="2 2 0.5"/>
    </visual>
  </link>
   
  <joint name="base_to_right_leg" type="fixed">
    <parent link="base_link"/>
    <child link="right_leg"/>
  </joint>
   
</robot>
